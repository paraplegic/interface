{
	"tag" : "root",
	"children" : [
		{	"tag" : "f0",
			"widget" : "ttk.LabelFrame",
			"args" : { "text" : "Top Frame" },
			"grid" : { "row" : "0", "column" : "0", "sticky" : "news" },
			"col_weight" : ["1"],
			"row_weight" : ["1"],
			"children" : [
				{	"tag" : "nb0",
					"widget" : "ttk.Notebook",
