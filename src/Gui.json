							"args" : { "text" : "Tab 0" },
							"add" : { "parent" : "nb0", "text" : "Tab 0" },
							"children" : [
								{	"tag" : "b0",
									"widget" : "ttk.Button",
									"args" : { "text" : "Exit", "command" : "self.exitApplication" },
