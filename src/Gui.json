								{	"tag" : "cnv1",
									"widget" : "Tk.Canvas",
									"grid" : { "row" : "0", "column" : "0", "sticky" : "news" },
									"scroll" : "xy",
									"bind" : { "event" : "<MouseWheel>", "binding" : "self.mousewheel" },
